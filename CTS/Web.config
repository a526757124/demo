      <!-- 不设置这里会提示找不到提供程序-->
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL" description="ADO.Net driver for MySQL" invariant="MySql.Data.MySqlClient" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data"/>
